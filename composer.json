{
    "name": "digitv/yii2sockets",
    "version": "1.0.6.3",
    "description": "Node.js web-sockets support",
    "keywords": ["yii2", "node", "nodejs", "web-sockets"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/digitv/yii2sockets/issues",
        "source": "https://github.com/digitv/yii2sockets"
    },
    "authors": [
        {
            "name": "Digit",
            "email": "digit.vova@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-redis": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "digitv\\yii2sockets\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}